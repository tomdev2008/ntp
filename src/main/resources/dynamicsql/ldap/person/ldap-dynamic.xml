<?xml version="1.0" encoding="utf-8"?>
<dynamic-hibernate-statement name="person">

    <!--根据组织架构编号查询-->
    <sql-query name="selectElementByKey">
        <![CDATA[
		SELECT FDID FROM SYS_ORG_ELEMENT WHERE FD_NO=:FD_NO
		]]>
    </sql-query>


    <!--根据组织架构编号查询-->
    <sql-query name="saveElement">
        <![CDATA[
		INSERT INTO SYS_ORG_DEPART(FDID,FD_LOGIN_NAME,FD_PASSWORD,FD_EMAIL,FDMOBILENO,FD_WORK_PHONE,FD_IDENTITY_CARD,FD_IS_EMP,DEPATID,FD_SEX)
		VALUES(:FDID,:FD_LOGIN_NAME,:FD_PASSWORD,:FD_EMAIL,:FDMOBILENO,:FD_WORK_PHONE,:FD_IDENTITY_CARD,:FD_IS_EMP,:DEPATID,:FD_SEX)
		]]>
    </sql-query>

</dynamic-hibernate-statement>